								{
									"name": "Add Employee",
									"event": [
										{
											"listen": "test",
											"script": {
												"exec": [
													"pm.test(\"Status code is 200\", function () {\r",
													"    pm.response.to.have.status(200);\r",
													"});\r",
													"\r",
													"pm.test(\"Response has employee id\", function () {\r",
													"    var jsonData = pm.response.json();\r",
													"    pm.expect(jsonData).to.have.property(\"id\");\r",
													"});\r",
													"\r",
													"pm.test(\"First and Last name match input\", function () {\r",
													"    var jsonData = pm.response.json();\r",
													"    pm.expect(jsonData.firstName).to.eql(pm.variables.get(\"firstName\"));\r",
													"    pm.expect(jsonData.lastName).to.eql(pm.variables.get(\"lastName\"));\r",
													"});\r",
													"\r",
													"pm.test(\"Benefits costs calculations are correct\", function () {\r",
													"    var jsonData = pm.response.json();\r",
													"    var actualCost = jsonData.benefitsCost;\r",
													"    var expectedCost = (1000+jsonData.dependants*500)/26;\r",
													"    var actualNet = jsonData.net;\r",
													"    var expectedNet = jsonData.gross - expectedCost;\r",
													"    pm.expect(Math.abs(actualCost - expectedCost)).to.be.below(0.0001);\r",
													"    pm.expect(Math.abs(actualNet - expectedNet)).to.be.below(0.0001);\r",
													"});\r",
													"\r",
													"// Save ID for later requests\r",
													"pm.environment.set(\"employeeId\", pm.response.json().id);\r",
													""
												],
												"type": "text/javascript",
												"packages": {}
											}
